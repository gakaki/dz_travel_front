#postal {
  top: 91rpx;
  width: 550rpx;
  height: 930rpx;
  background-color: #fff;
  box-shadow: 0 0 2rpx 4rpx rgba(0,0,0,.1);
  padding: 40rpx 30rpx 0 30rpx;
}
.hc {
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
}
#line {
  width: 12rpx;
  height: 170rpx;
  top: -60rpx;
  z-index: 12
}
.cir {
  top: 105rpx;
  z-index: 11;
  width: 16rpx;
  height: 16rpx;
  border-radius: 50%;
  background-color: #b9c3d0;
  box-shadow: 0 0 2rpx 0 inset
}
.post-pic {
 width: 550rpx;
  height: 565rpx;
  background-color: rgba(0,0,0,.1);
}
.post-name {
  font: bold 36rpx 'Microsoft YaHei';
  color: #000;
  margin: 20rpx 0;
}
.post-desc {
  font: bold 30rpx 'Microsoft YaHei';
  color: #8d8d8d;
  margin-bottom: 10rpx;
  letter-spacing: 2rpx
}
.btn-icon {
  margin-top: 44rpx;
  letter-spacing: 30rpx
}
.fc {
  font: bold 28rpx/40rpx 'Microsoft YaHei';
  color: #b8b8b8
}
.to-top {
  transform: rotate(180deg)
}
.play-pop {
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  border-radius: 40rpx;
  width: 610rpx;
  padding: 0 30rpx;
  background-color: #fff;
  box-shadow: 0 0 2rpx 4rpx rgba(0, 0, 0, 0.1);
  box-sizing: border-box;
}
.play-congratulations {
  border-radius: 20rpx;
  width: 610rpx;
  height: 1085rpx;
  padding: 15rpx 15rpx 0 15rpx;
  top: 20rpx;
  text-align: center;
}

.play-congratulations .imgBox {
  width: 580rpx;
  height: 440rpx;
  border-radius: 0;
  margin-bottom: 50rpx;
  position: relative;
}

.play-congratulations .imgBox image {
  width: 100%;
  height: 440rpx;
}

.play-congratulations .contentBox .congratulations-head-font {
  font-size: 62rpx;
  line-height: 62rpx;
  margin-bottom: 62rpx;
}

.play-congratulations .contentBox .congratulations-gray-font {
  font-size: 40rpx;
  line-height: 40rpx;
  margin-bottom: 175rpx;
}

.play-congratulations .contentBox .cfm-btn {
  margin: 0 auto;
  font-size: 28rpx;
  font-weight: normal;
}
.ten {
  width: 88rpx;
  height: 36rpx;
  background-color: rgba(0, 0, 0, 0.5);
  border-radius: 30rpx;
  position: absolute;
  right: 20rpx;
  bottom: 20rpx;
  font-size: 24rpx;
  line-height: 36rpx;
  color: #fff;
  text-align: center;
}
.head-font {
  font-family: 'Microsoft YaHei';
  display: block;
  font-size: 32rpx;
  line-height: 32rpx;
}

.gray-font {
  font-family: 'Microsoft YaHei';
  display: block;
  font-size: 28rpx;
  line-height: 28rpx;
  color: #8d8d8d;
}
.play-mission .contentBox .ml14 {
  margin-left: 14rpx;
}

.play-mission .contentBox .ml18 {
  margin-left: 18rpx;
  line-height: 40rpx;
}
