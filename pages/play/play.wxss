/* 基础pop样式 */

.play-pop {
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  border-radius: 40rpx;
  width: 610rpx;
  padding: 0 30rpx;
  background-color: #fff;
  box-shadow: 0 0 2rpx 4rpx rgba(0, 0, 0, 0.1);
  box-sizing: border-box;
}

.play-mission {
  border-radius: 20rpx;
  height: 590rpx;
  width: 560rpx;
  top: 220rpx;
  padding: 20rpx 20rpx 0 20rpx;
}

.play-mission-double {
  height: 710rpx;
}

.play-mission .imgBox {
  width: 520rpx;
  height: 280rpx;
