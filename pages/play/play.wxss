}

.play-rule-con {
  position: relative;
}

.play-rule-con::before {
  content: '';
  position: absolute;
  top: 0;
  left: -24rpx;
}

.rule-con {
  width: 500rpx;
  font: bold 24rpx/35rpx 'Microsoft YaHei';
  position: relative;
}

.rule-con::before {
  content: '';
  position: absolute;
  top: 0;
  left: -24rpx;
}

