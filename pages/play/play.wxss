  text-align: center;
}

.weather {
  width: 40rpx;
  height: 40rpx;
  border: solid 3rpx #fff;
  position: fixed;
  margin: 0 auto;
  left: 0;
  right: 0;
  top: 22rpx;
  border-radius: 3px;
}

.tip {
  background: url("https://gengxin.odao.com/update/h5/travel/play/tip.png") 0 0 no-repeat;
  background-size: cover;
  position: fixed;
  top: 20rpx;
  right: 20rpx;
